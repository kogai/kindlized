{
  "name": "kindlized",
  "version": "0.1.0",
  "description": "notification kindlized books.",
  "main": "app.js",
  "engines": {
    "node": "0.12.x"
  },
  "scripts": {
    "test": "env NODE_PATH=./ ./node_modules/.bin/mocha spec",
    "wip": "env NODE_PATH=./ ./node_modules/.bin/mocha spec/routes/search/amazon",
    "sandbox": "env NODE_PATH=./ node-dev --harmony --debug sandbox",
    "sbapi": "env NODE_PATH=./ node-dev --harmony --debug sbapi",
    "app": "env NODE_PATH=./ node-dev --debug bin/www",
    "shelf": "env NODE_PATH=./ node-dev --all-deps shelf",
    "librarian": "env NODE_PATH=./ node-dev --all-deps librarian",
    "db": "env NODE_PATH=./ node-dev --all-deps db",
    "postman": "env NODE_PATH=./ node-dev --all-deps postman",
    "build": "browserify -t debowerify -d public/javascripts/index.js -o public/javascripts/min/bundle.js",
    "start": "env NODE_PATH=./ PORT=80 pm2 start bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kogai/kindlized.git"
  },
  "bugs": {
    "url": "https://github.com/kogai/kindlized/issues"
  },
  "homepage": "https://github.com/kogai/kindlized",
  "author": "https://twitter.com/iamchawan",
  "license": "ISC",
  "dependencies": {
    "apac": "^1.0.0",
    "bcrypt": "^0.8.1",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "cron": "^1.0.5",
    "debug": "~2.0.0",
    "escape-regexp": "0.0.1",
    "express": "^4.9.0",
    "express-session": "^1.9.1",
    "formidable": "^1.0.15",
    "jade": "~1.6.0",
    "levenshtein": "^1.0.5",
    "log4js": "^0.6.22",
    "log4js-extend": "^0.1.2",
    "mocha": "^2.0.1",
    "moment-timezone": "^0.3.0",
    "mongoose": "^3.8.18",
    "mongoose-unique-validator": "^0.4.1",
    "morgan": "~1.3.0",
    "multer": "^0.1.6",
    "node-uuid": "^1.4.1",
    "nodemailer": "^1.3.0",
    "passport": "^0.2.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.2",
    "q": "^1.1.2",
    "serve-favicon": "~2.1.3",
    "socket.io": "^1.1.0",
    "underscore": "^1.8.2"
  },
  "devDependencies": {
    "browser-sync": "^2.0.1",
    "browserify": "^8.1.3",
    "chai": "^1.9.2",
    "coffee-script": "^1.9.0",
    "coffeeify": "^1.0.0",
    "debowerify": "^1.2.0",
    "gulp": "^3.8.10",
    "gulp-coffee": "^2.3.1",
    "gulp-coffeeify": "^0.1.2",
    "gulp-coffeelint": "^0.4.0",
    "gulp-data": "^1.2.0",
    "gulp-debug": "^2.0.0",
    "gulp-imagemin": "^2.2.0",
    "gulp-jade": "^0.11.0",
    "gulp-jshint": "^1.9.2",
    "gulp-minify-css": "^0.4.5",
    "gulp-newer": "^0.5.0",
    "gulp-pngmin": "^0.1.4",
    "gulp-sass": "^1.3.3",
    "gulp-shell": "^0.2.11",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-streamify": "0.0.5",
    "gulp-stylus": "^2.0.1",
    "gulp-uglify": "^1.1.0",
    "gulp-watch": "^4.1.1",
    "imagemin-jpegtran": "^4.1.0",
    "jshint-stylish": "^1.0.0",
    "licensify": "^1.0.0",
    "nib": "^1.1.0",
    "node-dev": "^2.6.0",
    "should": "^4.3.0",
    "supertest": "^0.15.0",
    "vinyl-source-stream": "^1.0.0"
  }
}
