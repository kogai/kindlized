{
  "name": "kindlized",
  "version": "0.1.0",
  "description": "notification kindlized books.",
  "main": "app.js",
  "engines": {
    "node": "4.1.2"
  },
  "scripts": {
    "start": "bin/www",
    "dev": "$(npm bin)/nodemon --legacy-watch bin/www",
    "gulp": "$(npm bin)/gulp",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test $(npm bin)/mocha --recursive --compilers js:espower-babel/guess",
    "test:unit:watch": "NODE_ENV=test $(npm bin)/mocha --recursive --watch --compilers js:espower-babel/guess",
    "eslint": "$(npm bin)/eslint .",
    "librarian": "NODE_PATH=./ $(npm bin)/node-dev Librarian/",
    "postman": "NODE_PATH=./ $(npm bin)/node-dev Postman/",
    "pm2:start": "$(npm bin)/pm2 start --no-daemon pm2.json",
    "pm2:restart": "$(npm bin)/pm2 restart",
    "pm2:info": "$(npm bin)/pm2 info",
    "pm2:list": "$(npm bin)/pm2 list",
    "pm2:stop": "$(npm bin)/pm2 stop",
    "pm2:logs": "$(npm bin)/pm2 logs",
    "pm2:restartall": "$(npm bin)/pm2 restart all",
    "pm2:stopall": "$(npm bin)/pm2 stop all",
    "pm2:kill": "$(npm bin)/pm2 kill"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kogai/kindlized.git"
  },
  "bugs": {
    "url": "https://github.com/kogai/kindlized/issues"
  },
  "homepage": "https://github.com/kogai/kindlized",
  "author": "https://twitter.com/iamchawan",
  "license": "ISC",
  "dependencies": {
    "apac": "^1.0.0",
    "babel": "^5.8.23",
    "babelify": "^7.2.0",
    "bcryptjs": "^2.1.0",
    "bluebird": "^2.10.2",
    "body-parser": "~1.8.1",
    "connect-redis": "^3.0.2",
    "cookie-parser": "~1.3.3",
    "cron": "^1.0.5",
    "debug": "~2.0.0",
    "deku": "^1.0.0",
    "email-templates": "^2.0.0-beta.1",
    "escape-regexp": "0.0.1",
    "express": "^4.9.0",
    "express-session": "^1.9.1",
    "express-sitemap": "^1.6.3",
    "formidable": "^1.0.15",
    "jade": "~1.6.0",
    "levenshtein": "^1.0.5",
    "lodash": "^3.10.1",
    "log4js": "^0.6.22",
    "log4js-extend": "^0.1.2",
    "mocha": "^2.0.1",
    "mockgoose": "^2.0.1",
    "moment-timezone": "^0.3.0",
    "mongoose": "^4.0.3",
    "mongoose-unique-validator": "^0.4.1",
    "morgan": "~1.3.0",
    "multer": "^0.1.6",
    "node-uuid": "^1.4.1",
    "nodemailer": "^1.3.0",
    "nodemailer-mandrill-transport": "^0.2.0",
    "object-assign": "^3.0.0",
    "passport": "^0.2.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.3",
    "pm2": "^0.15.7",
    "q": "^1.1.2",
    "redux": "^3.3.0",
    "serve-favicon": "~2.1.3",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "superagent": "^1.2.0",
    "twitter": "^1.2.5",
    "underscore": "^1.8.2",
    "validator": "^3.40.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.5.1",
    "espower-babel": "^3.3.0",
    "lodash": "^3.10.1",
    "nock": "^2.15.0",
    "nodemon": "^1.8.1",
    "page": "^1.6.4",
    "power-assert": "^1.1.0",
    "supertest": "^0.15.0",
    "supertest-session": "^1.1.2",
    "vinyl-source-stream": "^1.0.0",
    "webdriver-manager": "^7.0.1"
  }
}
